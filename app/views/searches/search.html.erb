<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <!--検索対象モデルがprefectureの時 -->

    <tbody>
      <% prefectures.each do |prefecture| %>
        <tr>
          <td><%= prefecture.name %></td>
        </tr>
      <% end %>
    </tbody>

    <!--検索対象モデルがgenreの時 -->
    <tbody>
      <% genres.each do |genre| %>
        <tr>
          <td><%= genre.name %></td>

        </tr>
      <% end %>
    </tbody>

</table>