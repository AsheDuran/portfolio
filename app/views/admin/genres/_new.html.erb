<h2>ジャンル追加</h2>
<div class="row">

  <%= form_with model: @genre, url: admin_genres_path do |f| %>
    <div class="col-auto" style="display:flex">
      <%= f.text_field :name, autofocus: true, placeholder:"新規ジャンルを入力 "%>
      <%= f.submit '新規登録',class:'btn btn-success ml-3' %>
    </div>
  <% end %>
</div>